{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "plugins": [
      {
        "name": "gql.tada/ts-plugin",
        "schema": "./src/graphql/introspection.graphql",
        "tadaOutputLocation": "./src/graphql/graphql-env.d.ts"
      }
    ]
  }
}